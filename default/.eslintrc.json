{
  "env": { "browser": true, "es2021": true },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": { "sourceType": "module", "ecmaVersion": "2021" },
  "ignorePatterns": [
    "/project_name/static/bundle.*",
    "/webpack.config.js",
    "staticfiles"
  ],
  "rules": { "no-console": "error" }
}
